{
  "name": "twilio-demo-bytes",
  "version": "0.0.1",
  "description": "Collection of snippets to live-demo the power of the Twilio platform",
  "workspaces": [
    "demos"
  ],
  "private": "true",
  "scripts": {
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "start": "npm run send-message",
    "conversations": "node demos/twilio/conversations",
    "count": "node demos/twilio/countMessagesCalls",
    "lookup": "node demos/twilio/lookup",
    "make-call": "node demos/twilio/makeCall",
    "send-message": "node demos/twilio/sendMessage",
    "verify": "node demos/twilio/verify",
    "verify-sna": "node demos/twilio/verify-sna",
    "send-email": "node demos/sendgrid/sendEmail",
    "send-advanced-email": "node demos/sendgrid/sendAdvancedEmail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IObert/twilio-demo-bytes.git"
  },
  "keywords": [
    "Twilio",
    "demo",
    "conference",
    "meetup"
  ],
  "author": "Marius Obert",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/IObert/twilio-demo-bytes/issues"
  },
  "homepage": "https://github.com/IObert/twilio-demo-bytes#readme",
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.3.0",
    "@sendgrid/mail": "^8.1.3",
    "axios": "^1.7.2",
    "datamask": "^1.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "ngrok": "^5.0.0-beta.2",
    "throttled-queue": "^2.1.4",
    "twilio": "^5.2.0"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "prettier": "^3.3.2"
  }
}
